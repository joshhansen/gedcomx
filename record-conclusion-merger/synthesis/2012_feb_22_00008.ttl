@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gdx: <http://example.com/genealogy#> .
@prefix users: <http://example.com/users#> .
@prefix records: <http://example.com/records#> .
@prefix : <http://example.com/synthesis/2012_feb_22_00008#> .

<> rdf:type gdx:Synthesis ;
	gdx:synthesisBy users:marysmith .

# Two people we're referencing from the transcriptions
:person1 rdf:type gdx:TranscribedPerson ;
	owl:sameAs <http://example.com/transcription/1930census_00221#person1> .

:person2 rdf:type gdx:TranscribedPerson ;
	owl:sameAs <http://example.com/transcription/1940census_00336#person1> .

# The synthesized person we're creating to represent Bob Jones.
:person3 rdf:type gdx:Person .

# Restate our source facts with proper attribution in case the original goes away
# (i.e. it’s on a system under someone else’s control)
:fact1 rdf:type gdx:TranscribedFact ;
	gdx:transcribedBy users:joeblogs ;
	gdx:transcriptionOf records:1930census00221 ;
	rdf:subject :person1 ;
	rdf:predicate gdx:age ;
	rdf:object [
		rdf:type Age ;
		atTime "1910" ;
		value "43y"
	] .

:fact2 rdf:type gdx:TranscribedFact ;
	gdx:transcribedBy users:janeblogs ;
	gdx:transcriptionOf records:1940census00336 ;
	rdf:subject :person2 ;
	rdf:predicate gdx:age ;
	rdf:object [
		rdf:type Age ;
		atTime "1920" ;
		value "53y"
	] .

:fact3 rdf:type gdx:SynthesizedFact ;
	gdx:synthesizedBy users:jorgegomez ;
	gdx:factCreationDate "5 Jan 2012" ;
	rdf:subject <http://example.com/transcription/1930census_00221#person1> ;
	rdf:predicate owl:sameAs ;
	rdf:object <http://example.com/transcription/1940census_00336#person3> .

# Assert some equivalences
:factA rdf:type gdx:SynthesizedFact ;
	gdx:supportingFact :fact3 ;
	rdf:subject :person3 ;
	rdf:predicate owl:sameAs ;
	rdf:object :person1 .

:factB rdf:type gdx:SynthesizedFact ;
	gdx:supportingFact :fact3 ;
	rdf:subject :person3 ;
	rdf:predicate owl:sameAs ;
	rdf:object :person2 .

# Deduce Bob Jones's birth date from the two age facts
:factC rdf:type gdx:SynthesizedFact ;
	gdx:factCreationDate "22 Feb 2012" ;
	gdx:supportingFact :fact1, :fact2, :fact3 ;
	rdf:subject :person3 ;
	rdf:predicate gdx:birthDate ;
	rdf:object "1867" .
